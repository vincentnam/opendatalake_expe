g the experiments."}], "pdbx_nmr_software": [{"authors": "KETCHEM,ROUX,CROSS", "classification": "refinement", "name": "TORC", "ordinal": 1, "version": "v5.4"}], "pdbx_nmr_spectrometer": [{"field_strength": 400.0, "manufacturer": "Home-built", "model": "Chemagnetics", "spectrometer_id": "1"}], "pdbx_vrpt_summary": {"angles_rmsz": 0.88, "attempted_validation_steps": "mogul,buster-report,molprobity,validation-pack,coreclust,chemicalshifts,percentiles,writepdf,writexml", "bonds_rmsz": 0.64, "clashscore_full_length": 0.0, "cyrange_error": "Not enough models (<3)", "nmr_models_consistency_flag": "True", "no_ligands_for_buster_report": "yes", "no_ligands_for_mogul": "yes", "no_percentile_property": "true", "num_hreduce": 55.0, "num_angles_rmsz": 170, "num_bonds_rmsz": 123, "panav_version": "Wang et al. (2010)", "percent_ramachandran_outliers_full_length": 0.0, "percentilebins": ["all", "nmr"], "protein_dnarnaentities": "1", "rci_version": "v_1n_11_5_13_A (Berjanski et al., 2005)", "report_creation_date": "Aug 20, 2022 -- 08:56 AM EDT", "shiftchecker_version": "2.29", "pdbdeposition_date": "2002-09-11T00:00:00+0000", "pdbrevision_date": "2022-02-23T00:00:00+0000", "pdbrevision_number": 4.0}, "rcsb_accession_info": {"deposit_date": "2002-09-11T00:00:00+0000", "has_released_experimental_data": "N", "initial_release_date": "2002-09-25T00:00:00+0000", "major_revision": 1, "minor_revision": 3, "revision_date": "2022-02-23T00:00:00+0000", "status_code": "REL"}, "rcsb_entry_container_identifiers": {"assembly_ids": ["1"], "entity_ids": ["1"], "entry_id": "1MP6", "model_ids": [1], "polymer_entity_ids": ["1"], "rcsb_id": "1MP6", "pubmed_id": 11604531}, "rcsb_entry_info": {"assembly_count": 1, "branched_entity_count": 0, "cis_peptide_count": 0, "deposited_atom_count": 124, "deposited_hydrogen_atom_count": 47, "deposited_model_count": 1, "deposited_modeled_polymer_monomer_count": 25, "deposited_nonpolymer_entity_instance_count": 0, "deposited_polymer_entity_instance_count": 1, "deposi